@{
ViewData["Title"] = "Dashbaord Page";
}

<div class="parent-data-class" , id="parent-data-id">
    <div class="row m-4">
        <!-- Card 2 -->

        <h3>Training Analytics: </h3>
        <div class="scrollable-div" id="analytics-card-body-id">

        </div>
        

        <h3>Saved Models</h3>
        <div class="scrollable-div">
            <input class="btn btn-secondary m-2" type="file" id="fileInput" multiple>
            <input type="text" readonly="readonly" id="fileName">

            <button class="btn btn-secondary m-2" id="uploadButton">Upload</button>
            <button class="btn btn-secondary m-2" id="downloadButton">Download</button>
        </div>

        <!-- Training data Card (with scrollable content) -->
        <br/>
        <h3>Training Runs</h3>
        <div class="scrollable-div" id="card-id">
        </div>
    </div>
</div>
