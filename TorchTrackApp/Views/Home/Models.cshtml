@{
    ViewData["Title"] = "Models Page";
}

<h3>Saved Models</h3>
<div class="scrollable-div">
    <input class="btn btn-secondary m-2" type="file" id="fileInput" multiple>
    <input type="text" readonly="readonly" id="fileName">

    <button class="btn btn-secondary m-2" id="uploadButton">Upload</button>
    <button class="btn btn-secondary m-2" id="downloadButton">Download</button>
</div>


